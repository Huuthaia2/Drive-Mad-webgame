var UI=(function(){
var root={};

root.init=function (){
    Main.init();
    Battle.init();
    Result.init();
    Sign.init();
    Match.init();
    Shop.init();
    AdView.init();
    Boss.init();
    if(Sign.checkCanSign()){
        Sign.show();
    }else{
        Main.show();
    }
}


return root;
})();
